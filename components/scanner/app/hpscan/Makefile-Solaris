Dist:: Target
	-@mkdir -p $(DistribBin)
	-@mkdir -p $(DistribLib)
	-@mkdir -p $(DistribLibExec)
	-@mkdir -p $(DistribDoc)
	-@mkdir -p $(DistribXbm)
	-@mkdir -p $(DistribPpm)
	-@mkdir -p $(DistribXpm)
	-@mkdir -p $(DistribGif)
	-@mkdir -p $(DistribBmp)
	@binaries="$(BINARIES)"			;\
	scripts="$(SCRIPTS)"			;\
	libraries="$(LIBRARIES)"		;\
	libexecs="$(LIBEXECS)"			;\
	documents="$(DOCUMENTS)"		;\
	images="$(IMAGES)"				;\
	for i in $$binaries				;\
	do								 \
	  echo "$$i	--> $(DistribBin)"	;\
	  cp $$i $(DistribBin)			;\
	  chmod ugo+rx $(DistribBin)/`basename $$i`	;\
	  $(STRIP) $(DistribBin)/`basename $$i`	;\
	done							;\
	for i in $$scripts				;\
	do								 \
	  echo "$$i	--> $(DistribBin)"	;\
	  cp $$i $(DistribBin)			;\
	  chmod ugo+rx $(DistribBin)/$$i	;\
	done							;\
	for i in $$libexecs				;\
	do								 \
	  echo "$$i	--> $(DistribLibExec)"	;\
	  cp $$i $(DistribLibExec)			;\
	  chmod ugo+rx $(DistribLibExec)/`basename $$i`	;\
	  $(STRIP) $(DistribLibExec)/`basename $$i`	;\
	done							;\
	for i in $$libraries			;\
	do								 \
	  cmd_cp="cp"					;\
	  cmd_chmod="chmod"				;\
	  if [ -d $$i ]					;\
	  then							 \
		cmd_cp="cp -r"				;\
		cmd_chmod="chmod -R"		;\
	  fi							;\
	  echo "$$i	--> $(DistribLib)"	;\
	  $$cmd_cp $$i $(DistribLib)			;\
	  $$cmd_chmod ugo+r $(DistribLib)/`basename $$i`	;\
	  $$cmd_chmod u+w	$(DistribLib)/`basename $$i`	;\
	done							;\
	for i in $$images				;\
	do								 \
	  case $$i in					 \
		*xbm) type=$(DistribXbm) ;;	\
		*msk) type=$(DistribXbm) ;;	\
		*ppm) type=$(DistribPpm) ;;	\
		*xpm) type=$(DistribXpm) ;;	\
		*gif) type=$(DistribGif) ;;	\
		*bmp) type=$(DistribBmp) ;;	\
		*)    echo "Unknown image type: $$i" ;type=$(DistribXbm) ;;	\
	  esac							;\
	  echo "$$i	--> $$type"			;\
	  cp $$i $$type					;\
	  chmod ugo+r $$type/`basename $$i`		;\
	  chmod u+w $$type/`basename $$i`			;\
	done							;\
	for i in $$documents			;\
	do								 \
	  echo "$$i	--> $(DistribDoc)"	;\
	  cp $$i $(DistribDoc)			;\
	  chmod ugo+r $(DistribDoc)/`basename $$i`	;\
	  chmod u+w $(DistribDoc)/`basename $$i`	;\
	done
	@$(MAKE) -f $(MKFILE) subDirs MK=Dist DRP=$(DRP)

