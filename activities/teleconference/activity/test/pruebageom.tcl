proc geo {w} {
   $w.l conf -text [wm geometry $w]
   after 1000 geo $w
}


proc Prueba {l} {

    set cont 0
    foreach v $l {
	
	puts $v
	
	toplevel .t$cont
	wm geometry .t$cont [lindex $v 1]


	label .t$cont.l
	pack .t$cont.l

	geo .t$cont
	
	incr cont

    }

    wm geometry . +1024+768

}


